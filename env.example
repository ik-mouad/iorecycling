# Keycloak
KC_ADMIN_USERNAME=admin
KC_ADMIN_PASSWORD=your_secure_password
KEYCLOAK_REALM=master
KEYCLOAK_CLIENT_ID=iorecycling-backend

# MinIO
MINIO_ROOT_USER=minio
MINIO_ROOT_PASSWORD=your_secure_password

# Frontend (Note: Angular utilise environment.prod.ts, ces variables sont pour référence)
# REACT_APP_API_URL=https://iorecycling.duckdns.org/api
# REACT_APP_KEYCLOAK_URL=https://iorecycling.duckdns.org/auth
# REACT_APP_KEYCLOAK_REALM=iorecycling
# REACT_APP_KEYCLOAK_CLIENT_ID=frontend

# Keycloak Hostname (pour production)
KC_HOSTNAME=iorecycling.duckdns.org
